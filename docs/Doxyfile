# Removes warning and Q_INVOKABLE flag for Q_INVOKABLE marked methods
MACRO_EXPANSION=YES
EXPAND_ONLY_PREDEF=YES
PREDEFINED= Q_INVOKABLE= \
            Q_GADGET=
HIDE_SCOPE_NAMES=YES

