# List the source input folders from which the documentation is generated here
INPUT=../include
RECURSIVE=YES
GENERATE_HTML=YES
GENERATE_XML=YES
XML_OUTPUT=xml
XML_PROGRAMLISTING=NO
CASE_SENSE_NAMES=NO
